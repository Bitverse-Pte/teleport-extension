{
  "name": "teleport-wallet",
  "version": "0.3.4",
  "description": "A multi-chain & multi-ID Wallet",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist && cp -r _raw/* dist",
    "clean:win": " fse emptyDir --quiet dist&&fse copy --all --keepExisting --dereference --errorOnExist --preserveTimestamps --quiet _raw dist",
    "build:dev:ui": "cross-env NODE_ENV=DEV webpack --progress --env config=dev --env module=ui",
    "build:tailwindcss": "npx tailwindcss -i ./src/ui/tailwind.css -o ./dist/tailwind.css",
    "build:dev:background": "webpack --progress --env config=dev --env module=background",
    "build:dev:contentScripts": "webpack --progress --env config=dev --env module=contentScripts",
    "build:dev": "cross-env NODE_ENV=DEV yarn clean && concurrently npm:build:tailwindcss npm:build:dev:ui npm:build:dev:background npm:build:dev:contentScripts",
    "build:pro:ui": "webpack --progress --env config=pro --env module=ui",
    "build:pro:background": "webpack --progress --env config=pro --env module=background",
    "build:pro:contentScripts": "webpack --progress --env config=pro --env module=contentScripts",
    "build:pro": "yarn clean && concurrently npm:build:tailwindcss npm:build:pro:ui npm:build:pro:background npm:build:pro:contentScripts",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "devtools:redux": "remotedev --hostname=localhost --port=8001",
    "prep:devtools:redux": "npm i -g remotedev-server --no-save",
    "test": "jest",
    "pub": "node build/release.js"
  },
